{
  "env": {
    "browser": true,
    "es2022": true,
    "jest": true,
    "node": true
  },
  "extends": ["airbnb", "airbnb-typescript"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "globalReturn": true,
      "impliedStrict": true
    },
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "rules": {
    "max-len": [
      "error", { "ignoreComments": true }
    ]
  }
}